{
  "name": "@featureflip-io/feature-flip-js",
  "version": "0.2.0",
  "description": "A client for the featureflip.io written in Typescript",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "umd:main": "dist/index.umd.js",
  "source": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/featureflip-io/feature-flip-js.git"
