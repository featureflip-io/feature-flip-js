    "@typescript-eslint/parser": "^4.29.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "microbundle": "^0.13.0",
    "nock": "^12.0.3",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "featureflip.io",
    "javascript",
    "typescript",
    "api",
    "client",
    "universal"
